<Window x:Class="NomadRecords.Meeting_Notes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Meeting Notes" Height="300" Width="300"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <GroupBox Header="Notes : " Height="245" Width="270" HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top">
            <TextBlock Name="NotesTextBlock" HorizontalAlignment="Left" Height="210" Width="250" TextWrapping="Wrap" Margin="5" VerticalAlignment="Top"/>
        </GroupBox>
    </Grid>
</Window>
